<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.view.controllers.MainScreenController">
  <children>
      <VBox alignment="CENTER">
         <children>
        <Label alignment="CENTER" style="&#10;" text="M.U.L.E." textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
            <Pane opacity="0.75" prefHeight="50.0" prefWidth="200.0" visible="false" />
        <Button fx:id="startButton" mnemonicParsing="false" onAction="#startGame" prefWidth="100.0" text="Start Game" />
            <Pane prefHeight="25.0" prefWidth="200.0" />
            <Button fx:id="loadButton" mnemonicParsing="false" onAction="#loadGame" prefWidth="100.0" text="Load Game" />
            <Pane prefHeight="25.0" prefWidth="200.0" />
        <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quitGame" prefWidth="100.0" text="Quit Game" />
         </children>
      </VBox>
  </children>
</VBox>
